@namespace Blatternfly.Components
@inherits BaseComponent

<thead class="@NoWrapClass" @attributes="AdditionalAttributes">
  @ChildContent
</thead>

@code {
  /// Won't wrap the table head if true.
  [Parameter] public bool NoWrap { get; set; }

  private string NoWrapClass { get => NoWrap ? "pf-m-nowrap" : null; }
}